---
import MainLayout from '../layouts/main-layout.astro';
---

<MainLayout title='Inspección'>
    <section id="mainSection" class="flex flex-col pt-[140px] md:flex-row pb-[30px]">
        <div class="w-full md:w-1/6 mr-4">
            <div class="bg-gray-100 shadow-md rounded-md p-6">
                <h2 class="text-center bonder mb-4 mt-4">Piroproceso</h2>
                <div id="list1" class="overflow-x-auto flex flex-row md:flex-col sm:flex-row">
                    <button class="oswald instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance1" data-target="ins_inter">Intercambiador</button>
                    <button class="oswald instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance2" data-target="ins_horn">Horno</button>
                    <button class="oswald instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance3" data-target="ins_quem">Quemador</button>
                    <button class="oswald instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance4" data-target="ins_enfri">Enfriador</button>
                </div>
                <h2 class="text-center bonder mb-4 mt-4">Loesche</h2>
                <div id="list2" class="overflow-x-auto flex flex-row md:flex-col sm:flex-row">
                    <button class="instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance5" data-target="ins_loesche_3">Molino 3</button>
                    <button class="instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance6" data-target="ins_loesche_7">Molino 7</button>
                    <button class="instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance7" data-target="ins_gen_mol">Inspeccion general de molino</button>
                    <button class="instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance8" data-target="ins_gen_clas">Inspeccion general de clasificador</button>
                </div>
                <h2 class="text-center bonder mb-4 mt-4">Molino de bola</h2>
                <div id="list3" class="overflow-x-auto flex flex-row md:flex-col sm:flex-row">
                    <button class="instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance9" data-target="ins_cam_sec">Camara de secado</button>
                    <button class="instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance10" data-target="ins_esp_ent">Espejo de entrada</button>
                    <button class="instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance11" data-target="ins_con_ent">Cono de entrada</button>
                    <button class="instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance12" data-target="ins_blin_1_cam">Blindaje 1° camara</button>
                    <button class="instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance13" data-target="ins_tab_cen">Tabique central</button>
                    <button class="instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance14" data-target="ins_blin_2_cam">Blindaje 2° camara</button>
                    <button class="instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance15" data-target="ins_tab_sal">Tabique de salida</button>
                    <button class="instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance16" data-target="ins_iny_agu_sal">Inyec. de agua a la salida</button>
                    <button class="instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance17" data-target="ins_air_fal">Aire falso</button>
                    <button class="instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance18" data-target="ins_ven_mol">Ventilacion del molino</button>
                    <button class="instance mb-1 w-full bg-blue-500 hover:bg-azul_yura text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline" id="instance19" data-target="ins_air_fal_2_pun">Aire falso entre dos puntos</button>
                </div>
            </div>
        </div>
        <div class="w-full md:w-5/6">
            <div class="bg-white-200 shadow-md rounded-md p-6 h-full overflow-y-auto">
                <div id="content"> 
                    <h1 class="oswald text-[25px] md:text-[30px] mt-[40px] mb-[30px] font-semibold text-center" >Bienvenido</h1>
                    <h2 class="oswald text-[18px] md:text-[20px] mb-[40px] text-center">Por favor selecione el apartado donde desee inicar la inspección</h2>
                </div> 
                <button id="enviarDatos" class="hidden oswald instance mt-[10px] mb-[20px] mb-1 w-full bg-purple-700 hover:bg-purple-800 text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline">Terminar inspeccion</button>
            </div>
        </div>
    </section>
 
    <!-- L1 -->
    <template id="ins_inter" class="hidden">
            <div class="header">
                <h2 class="text-lg font-semibold mb-4">Inspección de Intercambiador</h2>
            </div>
            <form class="formulario" id="formulario_inter">
                <label for="tem_riser">Temperatura del Riser</label>
                <div class="contenedor-input mt-2 mb-2"><input type="text" id="tem_riser" name="tem_riser" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
                <label for="tem_ent_cal">Temperatura Entrada al Calcinador</label>
                <div class="contenedor-input mt-2 mb-2"><input type="text" id="tem_ent_cal" name="tem_ent_cal" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
                <label for="tem_ter_ent_cal">Temperatura Aire Terciario Entrada Calcinador</label>
                <div class="contenedor-input mt-2 mb-2"><input type="text" id="tem_ter_ent_cal" name="tem_ter_ent_cal" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
                <label for="tem_cic_iv">T° Descarga Ciclon IV</label>
                <div class="contenedor-input mt-2 mb-2"><input type="text" id="tem_cic_iv" name="tem_cic_iv" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
                <label for="tem_gas_cic_iv">T° Gases Ciclon IV</label>
                <div class="contenedor-input mt-2 mb-2"><input type="text" id="tem_gas_cic_iv" name="tem_gas_cic_iv" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
                <label for="tem_cic_gem">T° Salida Ciclones Gemelos</label>
                <div class="contenedor-input mt-2 mb-2"><input type="text" id="tem_cic_gem" name="tem_cic_gem" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
                <label for="ana_sal_tor">Analizador Salida de la Torre</label>
                <div class="contenedor-input mt-2 mb-2"><input type="text" id="ana_sal_tor" name="ana_sal_tor" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
                <label for="vel_id_fan">Velocidad ID Fan</label>
                <div class="contenedor-input mt-2 mb-2"><input type="text" id="vel_id_fan" name="vel_id_fan" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
                <label for="pres_sal_gas_id">Presion Salida de Gases del ID</label>
                <div class="contenedor-input mt-2 mb-2"><input type="text" id="pres_sal_gas_id" name="pres_sal_gas_id" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
                <label for="dam_air_ter">Damper Aire Terciario</label>
                <div class="contenedor-input mt-2 mb-2"><input type="text" id="dam_air_ter" name="dam_air_ter" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
                <div class="contenedor-boton">
                    <button type="button" id="botonGuardar1" class="instance mt-3 mb-1 w-full bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline"> Guardar datos</button>
                </div>
            </form>
        <script>
            formulario1 = document.getElementById('formulario_inter');
            botonGuardar1 = document.getElementById('botonGuardar1');
            registrado1 = false;

            if (typeof tem_riserValue !== 'undefined') {document.getElementById('tem_riser').value = tem_riserValue;}
            if (typeof tem_ent_calValue !== 'undefined') {document.getElementById('tem_ent_cal').value = tem_ent_calValue;}
            if (typeof tem_ter_ent_calValue !== 'undefined') {document.getElementById('tem_ter_ent_cal').value = tem_ter_ent_calValue;}
            if (typeof tem_cic_ivValue !== 'undefined') {document.getElementById('tem_cic_iv').value = tem_cic_ivValue;}
            if (typeof tem_gas_cic_ivValue !== 'undefined') {document.getElementById('tem_gas_cic_iv').value = tem_gas_cic_ivValue;}
            if (typeof tem_cic_gemValue !== 'undefined') {document.getElementById('tem_cic_gem').value = tem_cic_gemValue;}
            if (typeof ana_sal_torValue !== 'undefined') {document.getElementById('ana_sal_tor').value = ana_sal_torValue;}
            if (typeof vel_id_fanValue !== 'undefined') {document.getElementById('vel_id_fan').value = vel_id_fanValue;}
            if (typeof pres_sal_gas_idValue !== 'undefined') {document.getElementById('pres_sal_gas_id').value = pres_sal_gas_idValue;}
            if (typeof dam_air_terValue !== 'undefined') {document.getElementById('dam_air_ter').value = dam_air_terValue;}

            if (localStorage.getItem('tem_riserValue')) {tem_riserValue = localStorage.getItem('tem_riserValue'); document.getElementById('tem_riser').value = tem_riserValue;}
            if (localStorage.getItem('tem_ent_calValue')) {tem_ent_calValue = localStorage.getItem('tem_ent_calValue'); document.getElementById('tem_ent_cal').value = tem_ent_calValue;}
            if (localStorage.getItem('tem_ter_ent_calValue')) {tem_ter_ent_calValue = localStorage.getItem('tem_ter_ent_calValue'); document.getElementById('tem_ter_ent_cal').value = tem_ter_ent_calValue;}
            if (localStorage.getItem('tem_cic_ivValue')) {tem_cic_ivValue = localStorage.getItem('tem_cic_ivValue'); document.getElementById('tem_cic_iv').value = tem_cic_ivValue;}
            if (localStorage.getItem('tem_gas_cic_ivValue')) {tem_gas_cic_ivValue = localStorage.getItem('tem_gas_cic_ivValue'); document.getElementById('tem_gas_cic_iv').value = tem_gas_cic_ivValue;}
            if (localStorage.getItem('tem_cic_gemValue')) {tem_cic_gemValue = localStorage.getItem('tem_cic_gemValue'); document.getElementById('tem_cic_gem').value = tem_cic_gemValue;}
            if (localStorage.getItem('ana_sal_torValue')) {ana_sal_torValue = localStorage.getItem('ana_sal_torValue'); document.getElementById('ana_sal_tor').value = ana_sal_torValue;}
            if (localStorage.getItem('vel_id_fanValue')) {vel_id_fanValue = localStorage.getItem('vel_id_fanValue'); document.getElementById('vel_id_fan').value = vel_id_fanValue;}
            if (localStorage.getItem('pres_sal_gas_idValue')) {pres_sal_gas_idValue = localStorage.getItem('pres_sal_gas_idValue'); document.getElementById('pres_sal_gas_id').value = pres_sal_gas_idValue;}
            if (localStorage.getItem('dam_air_terValue')) {dam_air_terValue = localStorage.getItem('dam_air_terValue'); document.getElementById('dam_air_ter').value = dam_air_terValue;}

            botonGuardar1.addEventListener('click', () => {
                const inputs = formulario1.querySelectorAll('input');
                
                if (registrado1) {
                    inputs.forEach(input => {
                        input.disabled = false;
                        input.style.backgroundColor = '';
                    });
                    botonGuardar1.style.backgroundColor = '';
                    botonGuardar1.textContent = 'Guardar datos';
                    botonGuardar1.addEventListener('mouseover', () => {botonGuardar1.style.backgroundColor = '';});
                    botonGuardar1.addEventListener('mouseout', () => {botonGuardar1.style.backgroundColor = '';});
                    document.getElementById('instance1').classList.remove('bg-blue-800');
                    document.getElementById('instance1').classList.add('bg-blue-500');
                } else {
                    inputs.forEach(input => {
                        input.disabled = true;
                        input.style.backgroundColor = '#f2f2f2';
                    });
                    botonGuardar1.style.backgroundColor = 'green';
                    botonGuardar1.textContent = 'Datos guardados!';
                    botonGuardar1.addEventListener('mouseover', () => {botonGuardar1.style.backgroundColor = '#006400';});
                    botonGuardar1.addEventListener('mouseout', () => {botonGuardar1.style.backgroundColor = 'green';});

                    tem_riserValue = formulario1.tem_riser.value,
                    tem_ent_calValue = formulario1.tem_ent_cal.value;
                    tem_ter_ent_calValue = formulario1.tem_ter_ent_cal.value;
                    tem_cic_ivValue = formulario1.tem_cic_iv.value;
                    tem_gas_cic_ivValue = formulario1.tem_gas_cic_iv.value;
                    tem_cic_gemValue = formulario1.tem_cic_gem.value;
                    ana_sal_torValue = formulario1.ana_sal_tor.value;
                    vel_id_fanValue = formulario1.vel_id_fan.value;
                    pres_sal_gas_idValue = formulario1.pres_sal_gas_id.value;
                    dam_air_terValue = formulario1.dam_air_ter.value;

                    localStorage.setItem('tem_riserValue', formulario1.tem_riser.value);
                    localStorage.setItem('tem_ent_calValue', formulario1.tem_ent_cal.value);
                    localStorage.setItem('tem_ter_ent_calValue', formulario1.tem_ter_ent_cal.value);
                    localStorage.setItem('tem_cic_ivValue', formulario1.tem_cic_iv.value);
                    localStorage.setItem('tem_gas_cic_ivValue', formulario1.tem_gas_cic_iv.value);
                    localStorage.setItem('tem_cic_gemValue', formulario1.tem_cic_gem.value);
                    localStorage.setItem('ana_sal_torValue', formulario1.ana_sal_tor.value);
                    localStorage.setItem('vel_id_fanValue', formulario1.vel_id_fan.value);
                    localStorage.setItem('pres_sal_gas_idValue', formulario1.pres_sal_gas_id.value);
                    localStorage.setItem('dam_air_terValue', formulario1.dam_air_ter.value);
 
                    document.getElementById('instance1').classList.remove('bg-blue-500');
                    document.getElementById('instance1').classList.add('bg-blue-800');
                }
                registrado1 = !registrado1;
            });
        </script>
    </template>

    <!-- L2 -->
    <template id="ins_horn" class="hidden">
        <div class="header">
            <h2 class="text-lg font-semibold mb-4">Inspección de Horno</h2>
        </div>
        <form class="formulario" id="formulario_horno">
            <label for="entrada_horno">Entrada al Horno</label>
            <div class="contenedor-input mt-2 mb-2"><input type="text" id="entrada_horno" name="entrada_horno" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
            <label for="grado_llenado_horno">Grado de Llenado del Horno</label>
            <div class="contenedor-input mt-2 mb-2"><input type="text" id="grado_llenado_horno" name="grado_llenado_horno" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
            <label for="oxigeno">Oxígeno</label>
            <div class="contenedor-input mt-2 mb-2"><input type="text" id="oxigeno" name="oxigeno" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
            <label for="co">CO</label>
            <div class="contenedor-input mt-2 mb-2"><input type="text" id="co" name="co" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
            <label for="presion_entrada_horno">Presión de Entrada al Horno</label>
            <div class="contenedor-input mt-2 mb-2"><input type="text" id="presion_entrada_horno" name="presion_entrada_horno" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
            <label for="alimentacion_horno">Alimentación al Horno</label>
            <div class="contenedor-input mt-2 mb-2"><input type="text" id="alimentacion_horno" name="alimentacion_horno" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
            <label for="aire_secundario">Aire Secundario</label>
            <div class="contenedor-input mt-2 mb-2"><input type="text" id="aire_secundario" name="aire_secundario" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
            <label for="zona_sinterizacion">Zona de Sinterización</label>
            <div class="contenedor-input mt-2 mb-2"><input type="text" id="zona_sinterizacion" name="zona_sinterizacion" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
            <label for="error_balanzas_carbon">Porcentaje Error de las Balanzas de Carbono</label>
            <div class="contenedor-input mt-2 mb-2"><input type="text" id="error_balanzas_carbon" name="error_balanzas_carbon" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
            <label for="inyeccion_carbon">Inyección de Carbono</label>
            <div class="contenedor-input mt-2 mb-2"><input type="text" id="inyeccion_carbon" name="inyeccion_carbon" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
            <label for="potencia_motor_horno">Potencia Motor del Horno</label>
            <div class="contenedor-input mt-2 mb-2"><input type="text" id="potencia_motor_horno" name="potencia_motor_horno" class="w-full border rounded px-3 py-2 focus:outline-none focus:border-blue-500 focus:ring focus:ring-blue-500 focus:ring-opacity-50"></div>
            <div class="contenedor-boton">
                <button type="button" id="botonGuardar2" class="oswald instance mt-3 mb-1 w-full bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 focus:outline-none focus:shadow-outline">Guardar datos</button>
            </div>
        </form>
        <script>
            formulario2 = document.getElementById('formulario_horno');
            botonGuardar2 = document.getElementById('botonGuardar2');
            registrado2 = false;
    
            if (typeof entrada_hornoValue !== 'undefined') {document.getElementById('entrada_horno').value = entrada_hornoValue;}
            if (typeof grado_llenado_hornoValue !== 'undefined') {document.getElementById('grado_llenado_horno').value = grado_llenado_hornoValue;}
            if (typeof oxigenoValue !== 'undefined') {document.getElementById('oxigeno').value = oxigenoValue;}
            if (typeof coValue !== 'undefined') {document.getElementById('co').value = coValue;}
            if (typeof presion_entrada_hornoValue !== 'undefined') {document.getElementById('presion_entrada_horno').value = presion_entrada_hornoValue;}
            if (typeof alimentacion_hornoValue !== 'undefined') {document.getElementById('alimentacion_horno').value = alimentacion_hornoValue;}
            if (typeof aire_secundarioValue !== 'undefined') {document.getElementById('aire_secundario').value = aire_secundarioValue;}
            if (typeof zona_sinterizacionValue !== 'undefined') {document.getElementById('zona_sinterizacion').value = zona_sinterizacionValue;}
            if (typeof error_balanzas_carbonValue !== 'undefined') {document.getElementById('error_balanzas_carbon').value = error_balanzas_carbonValue;}
            if (typeof inyeccion_carbonValue !== 'undefined') {document.getElementById('inyeccion_carbon').value = inyeccion_carbonValue;}
            if (typeof potencia_motor_hornoValue !== 'undefined') {document.getElementById('potencia_motor_horno').value = potencia_motor_hornoValue;}
            
            if (localStorage.getItem('entrada_hornoValue')) {entrada_hornoValue = localStorage.getItem('entrada_hornoValue');document.getElementById('entrada_horno').value = entrada_hornoValue;}
            if (localStorage.getItem('grado_llenado_hornoValue')) {grado_llenado_hornoValue = localStorage.getItem('grado_llenado_hornoValue');document.getElementById('grado_llenado_horno').value = grado_llenado_hornoValue;}
            if (localStorage.getItem('oxigenoValue')) {oxigenoValue = localStorage.getItem('oxigenoValue');document.getElementById('oxigeno').value = oxigenoValue;}
            if (localStorage.getItem('coValue')) {coValue = localStorage.getItem('coValue');document.getElementById('co').value = coValue;}
            if (localStorage.getItem('presion_entrada_hornoValue')) {presion_entrada_hornoValue = localStorage.getItem('presion_entrada_hornoValue');document.getElementById('presion_entrada_horno').value = presion_entrada_hornoValue;}
            if (localStorage.getItem('alimentacion_hornoValue')) {alimentacion_hornoValue = localStorage.getItem('alimentacion_hornoValue');document.getElementById('alimentacion_horno').value = alimentacion_hornoValue;}
            if (localStorage.getItem('aire_secundarioValue')) {aire_secundarioValue = localStorage.getItem('aire_secundarioValue');document.getElementById('aire_secundario').value = aire_secundarioValue;}
            if (localStorage.getItem('zona_sinterizacionValue')) {zona_sinterizacionValue = localStorage.getItem('zona_sinterizacionValue');document.getElementById('zona_sinterizacion').value = zona_sinterizacionValue;}
            if (localStorage.getItem('error_balanzas_carbonValue')) {error_balanzas_carbonValue = localStorage.getItem('error_balanzas_carbonValue');document.getElementById('error_balanzas_carbon').value = error_balanzas_carbonValue;}
            if (localStorage.getItem('inyeccion_carbonValue')) {inyeccion_carbonValue = localStorage.getItem('inyeccion_carbonValue');document.getElementById('inyeccion_carbon').value = inyeccion_carbonValue;}
            if (localStorage.getItem('potencia_motor_hornoValue')) {potencia_motor_hornoValue = localStorage.getItem('potencia_motor_hornoValue');document.getElementById('potencia_motor_horno').value = potencia_motor_hornoValue;}

            botonGuardar2.addEventListener('click', () => {
                const inputs = formulario2.querySelectorAll('input');
                
                if (registrado2) {
                    inputs.forEach(input => {
                        input.disabled = false;
                        input.style.backgroundColor = '';
